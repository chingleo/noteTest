journal = {Journal of Fluid Mechanics},
keywords = {b{\'{e}}nard convection,plumes,polymers,thermals},
pages = {R3},
title = {{Effects of polymer additives in the bulk of turbulent thermal convection}},
url = {http://www.journals.cambridge.org/abstract{\_}S0022112015006187},
