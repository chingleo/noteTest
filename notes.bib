keywords = {fully developed turbulence,laminar and turbulent boundary,layers,quasi-keplerian flows,rotating flow,transport properties},
number = {1},
pages = {53--80},
title = {{High–Reynolds Number Taylor-Couette Turbulence}},
volume = {48},
year = {2016}
}
