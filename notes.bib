@article{He2016,
abstract = {We present measurements of the orientation {\$}\backslashtheta{\_}0{\$} and temperature amplitude {\$}\backslashdelta{\$} of the large-scale circulation in a cylindrical sample of turbulent Rayleigh-Benard convection (RBC) with aspect ratio {\$}\backslashGamma \backslashequiv D/L = 1.00{\$} ({\$}D{\$} and {\$}L{\$} are the diameter and height respectively) and for the Prandtl number {\$}Pr \backslashsimeq 0.8{\$}. Results for {\$}\backslashtheta{\_}0{\$} revealed a preferred orientation with upflow in the West, consistent with a broken azimuthal invariance due to Earth's Coriolis force [see $\backslash$cite{\{}BA06b{\}}]. They yielded the azimuthal diffusivity {\$}D{\_}\backslashtheta{\$} and a corresponding Reynolds number {\$}Re{\_}{\{}\backslashtheta{\}}{\$} for Rayleigh numbers over the range {\$}2\backslashtimes 10{\^{}}{\{}12{\}} {\textless} Ra {\textless} 1.5\backslashtimes 10{\^{}}{\{}14{\}}{\$}. In the classical state ({\$}Ra {\textless} 2\backslashtimes 10{\^{}}{\{}13{\}}{\$}) the results were consistent with the measurements by $\backslash$cite{\{}BA06a{\}} for {\$}Ra {\textless} 10{\^{}}{\{}11{\}}{\$} and {\$}Pr = 4.38{\$} which gave {\$}Re{\_}{\{}\backslashtheta{\}} \backslashpropto Ra{\^{}}{\{}0.28{\}}{\$}, and with the Prandtl-number dependence {\$}Re{\_}{\{}\backslashtheta{\}} \backslashpropto Pr{\^{}}{\{}-1.2{\}}{\$} as found previously also for the velocity-fluctuation Reynolds number {\$}Re{\_}V{\$} $\backslash$cite[]{\{}HGBA15b{\}}. At larger {\$}Ra{\$} the data for {\$}Re{\_}{\{}\backslashtheta{\}}(Ra){\$} revealed a transition to a new state, known as the "ultimate" state, which was first seen in the Nusselt number {\$}Nu(Ra){\$} and in {\$}Re{\_}V(Ra){\$} at {\$}Ra{\^{}}*{\_}1 \backslashsimeq 2\backslashtimes 10{\^{}}{\{}13{\}}{\$} and {\$}Ra{\^{}}*{\_}2 \backslashsimeq 8\backslashtimes 10{\^{}}{\{}13{\}}{\$}. In the ultimate state we found {\$}Re{\_}{\{}\backslashtheta{\}} \backslashpropto Ra{\^{}}{\{}0.40\backslashpm 0.03{\}}{\$}. Recently $\backslash$cite{\{}SU15{\}} claimed that non-Oberbeck-Boussinesq effects on the Nusselt and Reynolds numbers of turbulent RBC may have been interpreted erroneously as a transition to a new state. We demonstrate that their reasoning is incorrect and that the transition observed in the G$\backslash$"ottingen experiments and discussed in the present paper is indeed to a new state of RBC referred to as "ultimate".},
