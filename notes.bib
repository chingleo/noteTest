file = {:C$\backslash$:/Users/chingleo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Shishkina, Weiss, Bodenschatz - 2016 - Conductive heat flux in measurements of the Nusselt number in turbulent Rayleigh-B{\'{e}}nard convecti.pdf:pdf},
journal = {Physical Review Fluids},
number = {6},
pages = {1--7},
title = {{Conductive heat flux in measurements of the Nusselt number in turbulent Rayleigh-B{\'{e}}nard convection}},
volume = {1},
year = {2016}
}
