file = {:C$\backslash$:/Users/chingleo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ostilla-Monico et al. - 2015 - A multiple-resolution strategy for Direct Numerical Simulation of scalar turbulence.pdf:pdf},
isbn = {00219991},
issn = {10902716},
journal = {Journal of Computational Physics},
keywords = {Direct Numerical Simulation,Multiple resolution,Scalar turbulence},
pages = {308--321},
publisher = {Elsevier Inc.},
title = {{A multiple-resolution strategy for Direct Numerical Simulation of scalar turbulence}},
url = {http://dx.doi.org/10.1016/j.jcp.2015.08.031},
volume = {301},
year = {2015}
}
@article{Pandey2014,
abstract = {Under the limit of infinite Prandtl number, we derive analytical expressions for the large-scale quantities, e.g., P{\'{e}}clet number Pe, Nusselt number Nu, and rms value of the temperature fluctuations $\theta$rms. We complement the analytical work with direct numerical simulations, and show that Nu∼Ra$\gamma$ with $\gamma$≈(0.30-0.32), Pe∼Ra$\eta$ with $\eta$≈(0.57-0.61), and $\theta$rms∼const. The Nusselt number is observed to be an intricate function of Pe, $\theta$rms, and a correlation function between the vertical velocity and temperature. Using the scaling of large-scale fields, we show that the energy spectrum Eu(k)∼k-13/3, which is in a very good agreement with our numerical results. The entropy spectrum E$\theta$(k), however, exhibits dual branches consisting of k-2 and k0 spectra; the k-2 branch corresponds to the Fourier modes $\theta${\^{i}}(0,0,2n), which are approximately -1/(2n$\pi$). The scaling relations for Prandtl number beyond 102 match with those for infinite Prandtl number. {\textcopyright} 2014 American Physical Society.},
archivePrefix = {arXiv},
arxivId = {arXiv:1305.1710v1},
author = {Pandey, Ambrish and Verma, Mahendra K. and Mishra, Pankaj K.},
