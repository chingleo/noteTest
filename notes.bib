@article{Guo2015,
abstract = {In the study, the influence of cell tilting on flow dynamics is explored experimentally in a rectangular cell (aspect ratios Gamma(x) = 1 and Gamma(y) = 0.25 ). The measurements are carried out in a wide range of tilt angles (0 {\textless}= beta {\textless}= pi/2 rad) at Prandtl number (Pr similar or equal to 6.3 ) and Rayleigh number (Ra similar or equal to 4.42 x 10(9) ). With the velocity measurements, the large-scale circulation (LSC) is found to be sensitive to the symmetry of the system. In the level case, the LSC is at about quarter width of the cell. As the cell is slightly tilted (beta similar or equal to 0.04 rad), the LSC moves quickly towards the boundary. With increasing beta , the LSC changes gradually from oblique ellipse-like to square-like, and to more complicated patterns. Oscillation has been found for almost all beta, and are strongest at around beta similar or equal to 0.48 rad. As beta increases, the Reynolds number (Re) initially also increases, until it reaches its maximum at the transition angle beta = 0 .15 rad, after which it gradually decreases. The cell tilting causes a pronounced reduction of the Nusselt number (Nu). As beta increases from 0 to 0.15, 1.05 and pi/2 rad, the reduction of Nu is approximately 1.4 {\%}, 5{\%} and 18 {\%}, respectively. Over the ranges of 0 {\textless}= beta {\textless}= 0.15 rad, 0.15 {\textless}= beta {\textless}= 1.05 rad and 1.05 {\textless}= beta {\textless}= pi/2 rad, the decay slopes are 8.57 x 10 (2), 3.27 x 10 (2) and 0.24 rad (-1), respectively.},
author = {Guo, Shuang-Xi and Zhou, Sheng-Qi and Cen, Xian-Rong and Qu, Ling and Lu, Yuan-Zheng and Sun, Liang and Shang, Xiao-Dong},
doi = {10.1017/jfm.2014.655},
file = {:C$\backslash$:/Users/chingleo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Guo et al. - 2015 - The effect of cell tilting on turbulent thermal convection in a rectangular cell.pdf:pdf},
isbn = {0022-1120},
issn = {0022-1120},
journal = {Journal of Fluid Mechanics},
number = {January},
pages = {273--287},
title = {{The effect of cell tilting on turbulent thermal convection in a rectangular cell}},
url = {http://www.journals.cambridge.org/abstract{\_}S0022112014006557},
volume = {762},
year = {2015}
}
@article{Guan2016,
abstract = {We report direct atomic-force-microscope measurements of capillary force hysteresis (CFH) and relaxation of a circular moving contact line (CL) formed on a long micron-sized hydrophobic fiber intersecting a liquid-air interface. By using 8 different liquid interfaces with varying solid-liquid molecular interactions, we find a universal behavior of the asymmetric speed-dependence of CFH and CL relaxation. A unified model based on force-assisted barrier-crossing is used to connect the mesoscopic measurements of CFH and CL relaxation with the energy barrier hight Eb and size $\lambda$ associated with the surface defects. The experiment demonstrates that the CL pinning (relaxation) and depinning dynamics are closely related and can be described by a common microscopic framework.},
