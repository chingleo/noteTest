publisher = {American Physical Society},
title = {{Controlling Heat Transport and Flow Structures in Thermal Turbulence Using Ratchet Surfaces}},
url = {https://doi.org/10.1103/PhysRevLett.120.044501},
volume = {120},
year = {2018}
}
