doi = {10.1017/jfm.2014.577},
file = {:C$\backslash$:/work{\_}space/refer{\_}paper/GoluskinD/GoluskinD14-JFM.pdf:pdf},
isbn = {0022-1120},
issn = {14697645},
journal = {Journal of Fluid Mechanics},
keywords = {B{\'{e}}nard convection,intermittency,plasmas},
number = {6},
pages = {360--385},
title = {{Convectively driven shear and decreased heat flux}},
volume = {759},
year = {2014}
}
