doi = {10.1103/PhysRevLett.104.104301},
file = {:C$\backslash$:/Users/chingleo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Zhou, Xia - 2010 - Measured instantaneous viscous boundary layer in turbulent rayleigh-b{\'{e}}nard convection.pdf:pdf},
