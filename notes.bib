file = {:C$\backslash$:/Users/chingleo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Chen et al. - 2017 - Theoretical and numerical study of enhanced heat transfer in partitioned thermal convection.pdf:pdf},
issn = {00179310},
journal = {International Journal of Heat and Mass Transfer},
keywords = {Forced convection,Heat transfer,Natural convection,Rayleigh-B{\'{e}}nard convection,Turbulence},
number = {October},
pages = {556--569},
publisher = {Elsevier Ltd},
title = {{Theoretical and numerical study of enhanced heat transfer in partitioned thermal convection}},
url = {http://dx.doi.org/10.1016/j.ijheatmasstransfer.2017.07.008},
