eprint = {arXiv:1408.1149},
file = {:C$\backslash$:/Users/chingleo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Schumacher et al. - 2014 - Small-scale universality in fluid turbulence.pdf:pdf},
isbn = {0027-8424},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
number = {30},
pages = {10961--10965},
pmid = {25024175},
title = {{Small-scale universality in fluid turbulence}},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.1410791111},
volume = {111},
year = {2014}
}
