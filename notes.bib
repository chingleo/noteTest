@article{Yadav2012,
abstract = {Using direct numerical simulations, we study dynamo action under Taylor-Green forcing for a magnetic Prandtl number of 0.5. We observe bistability with weak- and strong-magnetic-field branches. Both the dynamo branches undergo subcritical dynamo transition. We also observe a host of dynamo states including constant, periodic, quasiperiodic, and chaotic magnetic fields. One of the chaotic states originates through a quasiperiodic route with phase locking, while the other chaotic attractor appears to follow the Newhouse-Ruelle-Takens route to chaos. We also observe intermittent transitions between quasiperiodic and chaotic states for a given Taylor-Green forcing. {\textcopyright} 2012 American Physical Society.},
archivePrefix = {arXiv},
arxivId = {arXiv:1109.4894v1},
author = {Yadav, Rakesh K. and Verma, Mahendra K. and Wahi, Pankaj},
doi = {10.1103/PhysRevE.85.036301},
eprint = {arXiv:1109.4894v1},
file = {:C$\backslash$:/Users/chingleo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Yadav, Verma, Wahi - 2012 - Bistability and chaos in the Taylor-Green dynamo.pdf:pdf},
issn = {15393755},
journal = {Physical Review E - Statistical, Nonlinear, and Soft Matter Physics},
number = {3},
title = {{Bistability and chaos in the Taylor-Green dynamo}},
volume = {85},
year = {2012}
}
