@article{Wang2020,
abstract = {Recent findings suggest that wall-bounded turbulent flow can take different statistically stationary turbulent states, with different transport properties, even for the very same values of the control parameters. What state the system takes depends on the initial conditions. Here we analyze the multiple states in large-aspect ratio ({\$}\backslashGamma{\$}) two-dimensional turbulent Rayleigh--B$\backslash$'enard flow with no-slip plates and horizontally periodic boundary conditions as model system. We determine the number {\$}n{\$} of convection rolls, their mean aspect ratios {\$}\backslashGamma{\_}r = \backslashGamma /n{\$}, and the corresponding transport properties of the flow (i.e., the Nusselt number {\$}Nu{\$}), as function of the control parameters Rayleigh ({\$}Ra{\$}) and Prandtl number. The effective scaling exponent {\$}\backslashbeta{\$} in {\$}Nu \backslashsim Ra{\^{}}\backslashbeta{\$} is found to depend on the realized state and thus {\$}\backslashGamma{\_}r{\$}, with a larger value for the smaller {\$}\backslashGamma{\_}r{\$}. By making use of a generalized Friedrichs inequality, we show that the elliptical instability and viscous damping determine the {\$}\backslashGamma{\_}r{\$}-window for the realizable turbulent states. The theoretical results are in excellent agreement with our numerical finding {\$}2/3 \backslashle \backslashGamma{\_}r \backslashle 4/3{\$}, where the lower threshold is approached for the larger {\$}Ra{\$}. Finally, we show that the theoretical approach to frame {\$}\backslashGamma{\_}r{\$} also works for free-slip boundary conditions.},
author = {Wang, Qi and Verzicco, Roberto and Lohse, Detlef and Shishkina, Olga},
doi = {10.1103/physrevlett.125.074501},
file = {:C$\backslash$:/Users/chingleo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Wang et al. - 2020 - Multiple States in Turbulent Large-Aspect-Ratio Thermal Convection What Determines the Number of Convection Rolls.pdf:pdf},
issn = {0031-9007},
journal = {Physical Review Letters},
keywords = {doi:10.1103/PhysRevLett.125.074501 url:https://doi},
number = {7},
pages = {74501},
pmid = {32857539},
publisher = {American Physical Society},
title = {{Multiple States in Turbulent Large-Aspect-Ratio Thermal Convection: What Determines the Number of Convection Rolls?}},
url = {https://doi.org/10.1103/PhysRevLett.125.074501},
volume = {125},
year = {2020}
}
