journal = {Journal of Fluid Mechanics},
keywords = {B{\'{e}}nard convection,turbulent convection,variational methods},
pages = {1--12},
title = {{Bounds for Rayleigh–B{\'{e}}nard convection between free-slip boundaries with an imposed heat flux}},
volume = {837},
year = {2018}
}
