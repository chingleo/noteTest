keywords = {Conditioned long-short term memory,Implicit-explicit Runge-Kutta,Memory closure,Model reduction,POD-Galerkin},
pages = {109402},
publisher = {Elsevier Inc.},
title = {{Recurrent neural network closure of parametric POD-Galerkin reduced-order models based on the Mori-Zwanzig formalism}},
url = {https://doi.org/10.1016/j.jcp.2020.109402},
volume = {410},
