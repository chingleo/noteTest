isbn = {0022-1120 1469-7645},
issn = {14697645},
journal = {Journal of Fluid Mechanics},
keywords = {B??nard convection,Boundary layer structure,Turbulent convection},
pages = {344--373},
title = {{Local boundary layer scales in turbulent Rayleigh-B{\'{e}}nard convection}},
