doi = {10.1103/PhysRevE.79.026306},
file = {:C$\backslash$:/Users/chingleo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/He, Tong - 2009 - Measurements of the thermal dissipation field in turbulent Rayleigh-B{\'{e}}nard convection(2).pdf:pdf},
