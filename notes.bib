doi = {10.1063/1.4967495},
file = {:C$\backslash$:/Users/chingleo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Dabbagh et al. - 2016 - On the evolution of flow topology in turbulent Rayleigh-B{\'{e}}nard convection.pdf:pdf},
