file = {:C$\backslash$:/Users/chingleo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bhattacharya et al. - 2019 - Similarities between the structure functions of thermal convection and hydrodynamic turbulence.pdf:pdf},
issn = {10897666},
journal = {Physics of Fluids},
