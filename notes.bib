issn = {14697645},
journal = {Journal of Fluid Mechanics},
keywords = {B{\'{e}}nard convection,mantle convection,sea ice},
number = {March},
pages = {R11--R111},
title = {{Thermal convection with mixed thermal boundary conditions: Effects of insulating lids at the top}},
volume = {817},
year = {2017}
}
@article{Verschoof2018,
abstract = {We study periodically driven Taylor-Couette turbulence, i.e. the flow confined between two concentric, independently rotating cylinders. Here, the inner cylinder is driven sinusoidally while the outer cylinder is kept at rest (time-averaged Reynolds number is {\$}Re{\_}i = 5 \backslashtimes 10{\^{}}5{\$}). Using particle image velocimetry (PIV), we measure the velocity over a wide range of modulation periods {\$}T{\$}. To understand how the flow responds to a given modulation, we calculate the phase delay and amplitude response of the azimuthal velocity. In agreement with earlier theoretical and numerical work, we find that for large modulation periods the system follows the given modulation of the driving, i.e. the system behaves quasi-stationary. For smaller modulation periods, the flow cannot follow the modulation, and the flow velocity responds with a phase delay and a smaller amplitude response to the given modulation. Surprisingly, the local response in the bulk of the flow is independent of the distance to the modulated boundary. Apparently, the turbulent mixing is strong enough to prevent the flow from having radius-dependent responses to the given modulation.},
