@article{Zhu2016,
abstract = {We present direct numerical simulations of Taylor–Couette flow with grooved walls at a fixed radius ratio {\$}{\{}\backslashit\backslasheta{\}}=r{\_}{\{}i{\}}/r{\_}{\{}o{\}}=0.714{\$} with inner cylinder Reynolds number up to {\$}Re{\_}{\{}i{\}}=3.76\backslashtimes 10{\^{}}{\{}4{\}}{\$} , corresponding to Taylor number up to {\$}Ta=2.15\backslashtimes 10{\^{}}{\{}9{\}}{\$} . The grooves are axisymmetric V-shaped obstacles attached to the wall with a tip angle of 90°. Results are compared to the smooth wall case in order to investigate the effects of grooves on Taylor–Couette flow. We focus on the effective scaling laws for the torque, flow structures, and boundary layers. It is found that, when the groove height is smaller than the boundary layer thickness, the torque is the same as that of the smooth wall cases. With increasing {\$}Ta{\$} , the boundary layer thickness becomes smaller than the groove height. Plumes are ejected from the tips of the grooves and secondary circulations between the latter are formed. This is associated with a sharp increase of the torque, and thus the effective scaling law for the torque versus {\$}Ta{\$} becomes much steeper. Further increasing {\$}Ta{\$} does not result in an additional slope increase. Instead, the effective scaling law saturates to the ‘ultimate' regime effective exponents seen for smooth walls. It is found that even though after saturation the slope is the same as for the smooth wall case, the absolute value of torque is increased, and more so with the larger size of the grooves.},
author = {Zhu, Xiaojue and Ostilla-M{\'{o}}nico, Rodolfo and Verzicco, Roberto and Lohse, Detlef},
doi = {10.1017/jfm.2016.179},
file = {:C$\backslash$:/Users/chingleo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Zhu et al. - 2016 - Direct numerical simulation of Taylor-Couette flow with grooved walls Torque scaling and flow structure.pdf:pdf},
isbn = {0022-1120 1469-7645},
issn = {14697645},
journal = {Journal of Fluid Mechanics},
keywords = {Taylor-Couette flow,plumes/thermals,turbulence simulation},
pages = {746--774},
title = {{Direct numerical simulation of Taylor-Couette flow with grooved walls: Torque scaling and flow structure}},
volume = {794},
year = {2016}
}
@article{Zhang2018,
abstract = {{\textless}p{\textgreater} Rough surfaces have been widely used as an efficient way to enhance the heat-transfer efficiency in turbulent thermal convection. In this paper, however, we show that roughness does not always mean a heat-transfer enhancement, but in some cases it can also reduce the overall heat transport through the system. To reveal this, we carry out numerical investigations of turbulent Rayleigh–B{\'{e}}nard convection over rough conducting plates. Our study includes two-dimensional (2D) simulations over the Rayleigh number range {\textless}inline-formula{\textgreater} {\textless}alternatives{\textgreater} {\textless}inline-graphic href="S0022112017007868{\_}inline1" mime-subtype="gif" type="simple"/{\textgreater} {\textless}tex-math{\textgreater}{\$}10{\^{}}{\{}7{\}}\backslashleqslant Ra\backslashleqslant 10{\^{}}{\{}11{\}}{\$}{\textless}/tex-math{\textgreater} {\textless}/alternatives{\textgreater} {\textless}/inline-formula{\textgreater} and three-dimensional (3D) simulations at {\textless}inline-formula{\textgreater} {\textless}alternatives{\textgreater} {\textless}inline-graphic href="S0022112017007868{\_}inline2" mime-subtype="gif" type="simple"/{\textgreater} {\textless}tex-math{\textgreater}{\$}Ra=10{\^{}}{\{}8{\}}{\$}{\textless}/tex-math{\textgreater} {\textless}/alternatives{\textgreater} {\textless}/inline-formula{\textgreater} . The Prandtl number is fixed to {\textless}inline-formula{\textgreater} {\textless}alternatives{\textgreater} {\textless}inline-graphic href="S0022112017007868{\_}inline3" mime-subtype="gif" type="simple"/{\textgreater} {\textless}tex-math{\textgreater}{\$}Pr=0.7{\$}{\textless}/tex-math{\textgreater} {\textless}/alternatives{\textgreater} {\textless}/inline-formula{\textgreater} for both the 2D and the 3D cases. At a fixed Rayleigh number {\textless}inline-formula{\textgreater} {\textless}alternatives{\textgreater} {\textless}inline-graphic href="S0022112017007868{\_}inline4" mime-subtype="gif" type="simple"/{\textgreater} {\textless}tex-math{\textgreater}{\$}Ra{\$}{\textless}/tex-math{\textgreater} {\textless}/alternatives{\textgreater} {\textless}/inline-formula{\textgreater} , reduction of the Nusselt number {\textless}inline-formula{\textgreater} {\textless}alternatives{\textgreater} {\textless}inline-graphic href="S0022112017007868{\_}inline5" mime-subtype="gif" type="simple"/{\textgreater} {\textless}tex-math{\textgreater}{\$}Nu{\$}{\textless}/tex-math{\textgreater} {\textless}/alternatives{\textgreater} {\textless}/inline-formula{\textgreater} is observed for small roughness height {\textless}inline-formula{\textgreater} {\textless}alternatives{\textgreater} {\textless}inline-graphic href="S0022112017007868{\_}inline6" mime-subtype="gif" type="simple"/{\textgreater} {\textless}tex-math{\textgreater}{\$}h{\$}{\textless}/tex-math{\textgreater} {\textless}/alternatives{\textgreater} {\textless}/inline-formula{\textgreater} , whereas heat-transport enhancement occurs for large {\textless}inline-formula{\textgreater} {\textless}alternatives{\textgreater} {\textless}inline-graphic href="S0022112017007868{\_}inline7" mime-subtype="gif" type="simple"/{\textgreater} {\textless}tex-math{\textgreater}{\$}h{\$}{\textless}/tex-math{\textgreater} {\textless}/alternatives{\textgreater} {\textless}/inline-formula{\textgreater} . The crossover between the two regimes yields a critical roughness height {\textless}inline-formula{\textgreater} {\textless}alternatives{\textgreater} {\textless}inline-graphic href="S0022112017007868{\_}inline8" mime-subtype="gif" type="simple"/{\textgreater} {\textless}tex-math{\textgreater}{\$}h{\_}{\{}c{\}}{\$}{\textless}/tex-math{\textgreater} {\textless}/alternatives{\textgreater} {\textless}/inline-formula{\textgreater} , which is found to decrease with increasing {\textless}inline-formula{\textgreater} {\textless}alternatives{\textgreater} {\textless}inline-graphic href="S0022112017007868{\_}inline9" mime-subtype="gif" type="simple"/{\textgreater} {\textless}tex-math{\textgreater}{\$}Ra{\$}{\textless}/tex-math{\textgreater} {\textless}/alternatives{\textgreater} {\textless}/inline-formula{\textgreater} as {\textless}inline-formula{\textgreater} {\textless}alternatives{\textgreater} {\textless}inline-graphic href="S0022112017007868{\_}inline10" mime-subtype="gif" type="simple"/{\textgreater} {\textless}tex-math{\textgreater}{\$}h{\_}{\{}c{\}}\backslashsim Ra{\^{}}{\{}-0.6{\}}{\$}{\textless}/tex-math{\textgreater} {\textless}/alternatives{\textgreater} {\textless}/inline-formula{\textgreater} . Through dimensional analysis, we provide a physical explanation for this dependence. The physical reason for the {\textless}inline-formula{\textgreater} {\textless}alternatives{\textgreater} {\textless}inline-graphic href="S0022112017007868{\_}inline11" mime-subtype="gif" type="simple"/{\textgreater} {\textless}tex-math{\textgreater}{\$}Nu{\$}{\textless}/tex-math{\textgreater} {\textless}/alternatives{\textgreater} {\textless}/inline-formula{\textgreater} reduction is that the hot/cold fluid is trapped and accumulated inside the cavity regions between the rough elements, leading to a much thicker thermal boundary layer and thus impeding the overall heat flux through the system. {\textless}/p{\textgreater}},
