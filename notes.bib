@article{Skrbek2015,
abstract = {{\textless}p{\textgreater}An important question in turbulent Rayleigh–B{\'{e}}nard convection is the scaling of the Nusselt number with the Rayleigh number in the so-called ultimate state, corresponding to asymptotically high Rayleigh numbers. A related but separate question is whether the measurements support the so-called Kraichnan law, according to which the Nusselt number varies as the square root of the Rayleigh number (modulo a logarithmic factor). Although there have been claims that the Kraichnan regime has been observed in laboratory experiments with low aspect ratios, the totality of existing experimental results presents a conflicting picture in the high-Rayleigh-number regime. We analyse the experimental data to show that the claims on the ultimate state leave open an important consideration relating to non-Oberbeck–Boussinesq effects. Thus, the nature of scaling in the ultimate state of Rayleigh–B{\'{e}}nard convection remains open.{\textless}/p{\textgreater}},
author = {Skrbek, L. and Urban, P.},
doi = {10.1017/jfm.2015.638},
file = {:C$\backslash$:/Users/chingleo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Skrbek, Urban - 2015 - Has the ultimate state of turbulent thermal convection been observed.pdf:pdf},
isbn = {0022-1120$\backslash$r1469-7645},
issn = {14697645},
journal = {Journal of Fluid Mechanics},
keywords = {turbulent convection,turbulent flows},
pages = {270--282},
title = {{Has the ultimate state of turbulent thermal convection been observed?}},
volume = {785},
year = {2015}
}
@article{Zhu2017a,
abstract = {Direct numerical simulations (DNS) are performed to analyse the global transport properties of turbulent Taylor–Couette flow with inner rough wall up to Taylor number {\$}Ta=10{\^{}}{\{}10{\}}{\$} . The dimensionless torque {\$}Nu{\_}{\{}\backslashunicode[STIX]{\{}x1D714{\}}{\}}{\$} shows an effective scaling of {\$}Nu{\_}{\{}\backslashunicode[STIX]{\{}x1D714{\}}{\}}\backslashpropto Ta{\^{}}{\{}0.42\backslashpm 0.01{\}}{\$} , which is steeper than the ultimate regime effective scaling {\$}Nu{\_}{\{}\backslashunicode[STIX]{\{}x1D714{\}}{\}}\backslashpropto Ta{\^{}}{\{}0.38{\}}{\$} seen for smooth inner and outer walls. It is found that at the inner rough wall, the dominant contribution to the torque comes from the pressure forces on the radial faces of the rough elements; while viscous shear stresses on the rough surfaces contribute little to {\$}Nu{\_}{\{}\backslashunicode[STIX]{\{}x1D714{\}}{\}}{\$} . Thus, the log layer close to the rough wall depends on the roughness length scale, rather than on the viscous length scale. We then separate the torque contributed from the smooth inner wall and the rough outer wall. It is found that the smooth wall torque scaling follows {\$}Nu{\_}{\{}s{\}}\backslashpropto Ta{\_}{\{}s{\}}{\^{}}{\{}0.38\backslashpm 0.01{\}}{\$} , in excellent agreement with the case where both walls are smooth. In contrast, the rough wall torque scaling follows {\$}Nu{\_}{\{}r{\}}\backslashpropto Ta{\_}{\{}r{\}}{\^{}}{\{}0.47\backslashpm 0.03{\}}{\$} , very close to the pure ultimate regime scaling {\$}Nu{\_}{\{}\backslashunicode[STIX]{\{}x1D714{\}}{\}}\backslashpropto Ta{\^{}}{\{}1/2{\}}{\$} . The energy dissipation rate at the wall of an inner rough cylinder decreases significantly as a consequence of the wall shear stress reduction caused by the flow separation at the rough elements. On the other hand, the latter shed vortices in the bulk that are transported towards the outer cylinder and dissipated. Compared to the purely smooth case, the inner wall roughness renders the system more bulk dominated and thus increases the effective scaling exponent.},
author = {Zhu, Xiaojue and Verzicco, Roberto and Lohse, Detlef},
doi = {10.1017/jfm.2016.815},
file = {:C$\backslash$:/Users/chingleo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Zhu, Verzicco, Lohse - 2017 - Disentangling the origins of torque enhancement through wall roughness in Taylor-Couette turbulence.pdf:pdf},
