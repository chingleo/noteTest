doi = {10.1175/JPO-D-15-0126.1},
file = {:C$\backslash$:/Users/chingleo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Keitzl, Mellado, Notz - 2016 - Impact of Thermally Driven Turbulence on the Bottom Melting of Ice.pdf:pdf},
isbn = {978-989-8425-76-8},
issn = {0022-3670},
journal = {Journal of Physical Oceanography},
number = {4},
pages = {1171--1187},
title = {{Impact of Thermally Driven Turbulence on the Bottom Melting of Ice}},
url = {http://journals.ametsoc.org/doi/10.1175/JPO-D-15-0126.1},
volume = {46},
