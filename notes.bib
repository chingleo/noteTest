title = {{Thermal forcing and ‘classical' and ‘ultimate' regimes of Rayleigh–B{\'{e}}nard convection}},
url = {https://www.cambridge.org/core/product/identifier/S0022112019001186/type/journal{\_}article},
volume = {868},
year = {2019}
}
