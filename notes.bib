abstract = {We report an experimental study of the properties of the velocity boundary layer in turbulent Rayleigh-Benard convection in a cylindrical cell. The measurements were made at Rayleigh numbers R a in the range 2.4 x 10(8) {\textless} Ra {\textless} 5.6 x 10(9) and were conducted with the convection cell tilted with an angle theta relative to gravity, at theta = 0.5, 1.0, 2.0 and 3.4 degrees, respectively. The fluid was water with Prandtl number Pr = 5.3. It is found that at small tilt angles (theta {\textless}= 1 degrees), the measured viscous boundary layer thickness delta(nu) scales with the Reynolds number Re with an exponent close to that for a Prandtl-Blasius (PB) laminar boundary layer, i.e. delta(nu) similar to Re-0.46 +/- 0.03. For larger tilt angles, the scaling exponent of delta(nu) with Re decreases with theta. The normalized mean horizontal velocity profiles measured at the same tilt angle but with different Ra are found to have an invariant shape. However, for different tilt angles, the shape of the normalized profiles is different. It is also found that the Reynolds number R e based on the maximum mean horizontal velocity scales with Ra as Re similar to Ra-0.43 and the Reynolds number Re-sigma based on the maximum root mean square velocity scales with Ra as Re-sigma similar to Ra-0.55. Within the measurement resolution neither exponent depends on the tilt angle theta. Several wall quantities are also measured directly and their dependencies on R e are found to agree well with those predicted for a classical laminar boundary layer. These are the wall shear stress tau (similar to Re-1.46), the viscous sublayer delta(w) (similar to Re-0.75), the friction velocity u(tau) (similar to Re-0.86) and the skin friction coefficient c(f) (similar to Re-0.46). Again, all of these near-wall quantities do not exhibit a dependence on the tilt angle within the measurement resolution. We also examined the dynamical scaling method proposed by Zhou and Xia (Phys. Rev. Lett., vol. 104, 2010, p. 104301) and found that in both the laboratory and the dynamical frames the mean velocity profiles show deviations from the theoretical PB profile, with the deviations increasing with Ra. However, profiles obtained from dynamical scaling in general have better agreement with the theoretical profile. It is also found that the effectiveness of this method appears to be independent of Ra.},
author = {Wei, Ping and Xia, Ke Qing},
doi = {10.1017/jfm.2013.17},
file = {:C$\backslash$:/Users/chingleo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Wei, Xia - 2013 - Viscous boundary layer properties in turbulent thermal convection in a cylindrical cell The effect of cell tilting.pdf:pdf},
isbn = {0022-1120},
issn = {00221120},
journal = {Journal of Fluid Mechanics},
keywords = {buoyant boundary layers,turbulent convection,turbulent flows},
pages = {140--168},
title = {{Viscous boundary layer properties in turbulent thermal convection in a cylindrical cell: The effect of cell tilting}},
volume = {720},
year = {2013}
}
@article{Wei2012,
abstract = {We present an experimental study of turbulent Rayleigh-Benard convection with polymer additives made in two convection cells, one with a smooth top and bottom plates and the other with a rough top and bottom plates. For the cell with smooth plates, a reduction of the measured Nusselt number (Nu) was observed. Furthermore, the amount of Nu reduction increases with increasing polymer concentration (c), reaching 12{\%} for c=120 ppm and an apparent leveling off thereafter. For the cell with rough plates, however, an enhancement (4{\%}) of Nu was observed when the polymer concentration is greater than 120 ppm. This increase in Nu is corroborated by an increased large-scale circulation (LSC) velocity in the same cell when polymers are added. In contrast, the LSC velocity in the smooth cell is found to be essentially the same with and without polymers. It is further found that in the smooth cell the rms values of the global Nu, Nu , and that of the local temperature, T , both exhibit similar dependence on c as Nu itself. In contrast, Nu and T in the rough cell are found to be essentially independent of c. 2012 American Physical Society.},
author = {Wei, Ping and Ni, Rui and Xia, Ke Qing},
doi = {10.1103/PhysRevE.86.016325},
file = {:C$\backslash$:/Users/chingleo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Wei, Ni, Xia - 2012 - Enhanced and reduced heat transport in turbulent thermal convection with polymer additives.pdf:pdf},
isbn = {1539-3755$\backslash$r1550-2376},
issn = {15393755},
journal = {Physical Review E - Statistical, Nonlinear, and Soft Matter Physics},
number = {1},
pages = {1--13},
title = {{Enhanced and reduced heat transport in turbulent thermal convection with polymer additives}},
volume = {86},
year = {2012}
}
