issn = {14697645},
journal = {Journal of Fluid Mechanics},
keywords = {Taylor-Couette flow,rotating turbulence,turbulent flows},
pages = {834--845},
title = {{Periodically driven Taylor-Couette turbulence}},
volume = {846},
year = {2018}
}
