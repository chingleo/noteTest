journal = {Physical Review E},
number = {4},
pages = {1--12},
pmid = {26919000},
title = {{Simultaneous observation of asymmetric speed-dependent capillary force hysteresis and slow relaxation of a suddenly stopped moving contact line}},
volume = {94},
year = {2016}
}
