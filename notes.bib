keywords = {applications in,fluid,immersed,immersed boundary method,immersed finite-element method,interface method,medicine and biology,structure interaction},
number = {1},
pages = {421--448},
title = {{Immersed Methods for Fluid–Structure Interaction}},
volume = {52},
year = {2020}
}
@article{Bose2018,
