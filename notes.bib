eprint = {arXiv:1511.04862v1},
file = {:C$\backslash$:/Users/chingleo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Xie, Xia - 2013 - Dynamics and flow coupling in two-layer turbulent thermal convection.pdf:pdf},
isbn = {0022-1120},
