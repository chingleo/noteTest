doi = {10.1103/PhysRevFluids.2.062801},
file = {:C$\backslash$:/Users/chingleo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Castaing et al. - 2017 - Turbulent heat transport regimes in a channel.pdf:pdf},
issn = {2469990X},
journal = {Physical Review Fluids},
number = {6},
pages = {1--10},
title = {{Turbulent heat transport regimes in a channel}},
volume = {2},
year = {2017}
}
@article{Teimurazov2017,
abstract = {The turbulent convection of low-Prandtl-number fluids (Pr=0.0083) in a long cylindrical cell, heated at one end face and cooled at the other, inclined to the vertical at angle $\beta$, 0≤$\beta$≤$\pi$/2 with step $\pi$/20, is studied numerically by solving the Oberbeck-Boussinesq equations with the large-eddy-simulation approach for small-scale turbulence. The cylinder length is L=5D, where D is the diameter. The Rayleigh number, determined by the cylinder diameter, is of the order of 5×106. We show that the structure of the flow strongly depends on the inclination angle. A stable large-scale circulation (LSC) slightly disturbed by small-scale turbulence exists in the horizontal cylinder. The deviation from a horizontal position provides strong amplification of both LSC and small-scale turbulence. The energy of turbulent pulsations increases monotonically with decreasing inclination angle $\beta$, matching the energy of the LSC at $\beta$≈$\pi$/5. The intensity of the LSC has a wide, almost flat, maximum for an inclined cylinder and slumps approaching the vertical position, in which the LSC vanishes. The dependence of the Nusselt number on the inclination angle has a maximum at $\beta$≈7$\pi$/20 and generally follows the dependence of the intensity of LSC on the inclination. This indicates that the total heat transport is highly determined by LSC. We examine the applicability of idealized thermal boundary conditions (BCs) for modeling a real experiment with liquid sodium flows. Therefore, the simulations are done with two types of temperature BCs: fixed face temperature and fixed heat flux. The intensity of the LSC is slightly higher in the latter case and leads to a corresponding increase of the Nusselt number and enhancement of temperature pulsations.},
author = {Teimurazov, Andrei and Frick, Peter},
doi = {10.1103/PhysRevFluids.2.113501},
file = {:C$\backslash$:/Users/chingleo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Teimurazov, Frick - 2017 - Thermal convection of liquid metal in a long inclined cylinder.pdf:pdf},
issn = {2469990X},
journal = {Physical Review Fluids},
number = {11},
pages = {1--13},
title = {{Thermal convection of liquid metal in a long inclined cylinder}},
volume = {2},
year = {2017}
}
@article{Guervilly2017,
abstract = {One of the most prominent dynamical features of turbulent, rapidly rotating convection is the formation of large-scale coherent structures, driven by Reynolds stresses resulting from the small-scale convective flows. In spherical geometry, such structures consist of intense zonal flows that are invariant along the rotation axis. In planar geometry, long-lived, depth-invariant structures also form at large scales, but, in the absence of horizontal anisotropy, they consist of vortices that grow to the domain size. In this work, through the introduction of horizontal anisotropy into a numerical model of planar rotating convection by the adoption of unequal horizontal box sizes (i.e., Lx≤Ly, where the xy plane is horizontal), we investigate whether unidirectional flows and large-scale vortices can coexist. We find that only a small degree of anisotropy is required to bring about a transition from dynamics dominated by persistent large-scale vortices to dynamics dominated by persistent unidirectional flows parallel to the shortest horizontal direction. When the anisotropy is sufficiently large, the unidirectional flow consists of multiple jets, generated on a time scale smaller than a global viscous time scale, thus signifying that the upscale energy transfer does not spontaneously feed the largest available mode in the system. That said, the multiple jets merge on much longer time scales. Large-scale vortices of size comparable with Lx systematically form in the flanks of the jets and can be persistent or intermittent. This indicates that large-scale vortices, either coexisting with jets or not, are a robust dynamical feature of planar rotating convection.},
author = {Guervilly, C{\'{e}}line and Hughes, David W.},
doi = {10.1103/PhysRevFluids.2.113503},
file = {:C$\backslash$:/Users/chingleo/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Guervilly, Hughes - 2017 - Jets and large-scale vortices in rotating Rayleigh-B{\'{e}}nard convection.pdf:pdf},
issn = {2469990X},
journal = {Physical Review Fluids},
number = {11},
pages = {1--21},
title = {{Jets and large-scale vortices in rotating Rayleigh-B{\'{e}}nard convection}},
volume = {2},
year = {2017}
}
@article{Chatzidimitriou-Dreismann2000,
abstract = {MAv 15, 1962 micron thick. For an absorption coefficient of 0.5 cm ', the fraction of incident light absorbed in this thickness is 6.5x10, and the upper limit for the quantum yield of photovoltaic current pro-duced by two-quantum transitions would be half this figure, or 3.3 x10"'. On the other hand, the observed quantum yield for light of 1.0-micron wavelength is 1 x10 '. (Quantum yield is here defined as the. number of electrons flowing in the external circuit per photon incident on the tin.) Effects which might be due to stray light of higher energies may be ruled out by the observation that placing the gallium arsenide filter in the light path does not change the shape of the spectral response curve. A Fowler plot" has been made of the data for the range 0.88 to 1.30 eV. There is a good fit between the experimental data and the theoretical function for a barrier height of 0.75 eV in agree-ment with the other estimates of the barrier height mentioned above. The author is indebted to A. M. Goodman for carrying out the measurements of barrier ca-pacitance and to J. Blanc for several helpful discussions. Optical second harmonic generation has been observed as a function of an applied dc electric field in the crystal calcite, which possesses a center of inversion. Of particular significance, a small amount of doubling was observed to oc-cur with no dc electric field applied. The elec-tric-field-induced doubling and the doubling in crystals lacking a center of inversion can be un-derstood in terms of a three-photon process: the electric-dipole absorption of two similar photons followed by the electric-dipole emission of a third photon of twice the energy. ' We be-lieve the doubling, which we observe in calcite with no dc electric field applied, to be associated with the next order 3-photon process which in-volves an electric-quadrupole transition. We have also observed optical third harmonic gener-ation in calcite. The focused output beam of a one-joule pulsed ruby laser was used for all of the experiments reported here. Electrodes were imbedded in the calcite crystals for the electric-field-induced experiments. Results for a carefully prepared sample are shown in Fig. 1. Note that the second harmonic signal persists at zero applied field. The upper plot is included to show that the second harmonic generation is composed of two parts: the field-induced part with a light amplitude pro-portional to the applied field and a second part which adds to the first in quadrature. The data in Fig. 1 were taken with the laser bream incident at -27' to the optic axis and the laser polarized so that it passed through the crystal as an ordinary ray. At this angle the index of the ordinary ray at the laser wavelength is equal to that of the extraordinary ray at the second harmonic wavelength. '{\~{}}' The sharp angu-lar dependence of the output intensity reported previously for this type of index matching was observed. Somewhat weaker, second harmonic generation was also observed to occur at -37 to the optic axis with the laser polarized so that one has both ordinary and extraordinary laser light in the crystal. At this angle the indices are matched for combining an ordinary red ray with an extraordinary red ray to produce a sec-ond harmonic blue extraordinary ray. Second harmonic generation at the surfaces coherent with the bulk electric-field-induced second harmonic generation was observed. These 404},
author = {Chatzidimitriou-Dreismann, C. A. and Abdul-Redah, T. and Kolaric, B. and Juranic, I.},
