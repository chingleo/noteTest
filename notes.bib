issn = {15502376},
journal = {Physical Review E - Statistical, Nonlinear, and Soft Matter Physics},
number = {2},
pages = {1--12},
title = {{Scaling of heat flux and energy spectrum for very large Prandtl number convection}},
volume = {89},
year = {2014}
}
@article{Ostilla-Monico2016,
