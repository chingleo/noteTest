@article{Stevens2013,
abstract = {The unifying theory of scaling in thermal convection (Grossmann {\&} Lohse, J. Fluid. Mech., vol. 407, 2000, pp. 27–56; henceforth the GL theory) suggests that there are no pure power laws for the Nusselt and Reynolds numbers as function of the Rayleigh and Prandtl numbers in the experimentally accessible parameter regime. In Grossmann {\&} Lohse (Phys. Rev. Lett., vol. 86, 2001, pp. 3316–3319) the dimensionless parameters of the theory were fitted to 155 experimental data points by Ahlers {\&} Xu (Phys. Rev. Lett., vol. 86, 2001, pp. 3320–3323) in the regime $\backslash${\$}3\backslashtimes 1{\{}0{\}}{\^{}}{\{}7{\}} \backslashleq \backslashmathit{\{}Ra{\}}\backslashleq 3\backslashtimes 1{\{}0{\}}{\^{}}{\{}9{\}} \backslash{\$} and $\backslash${\$}4\backslashleq \backslashmathit{\{}Pr{\}}\backslashleq 34\backslash{\$} and Grossmann {\&} Lohse (Phys. Rev. E, vol. 66, 2002, p. 016305) used the experimental data point from Qiu {\&} Tong (Phys. Rev. E, vol. 64, 2001, p. 036304) and the fact that $\backslash${\$}\backslashmathit{\{}Nu{\}}(\backslashmathit{\{}Ra{\}}, \backslashmathit{\{}Pr{\}})\backslash{\$} is independent of the parameter $\backslash${\$}a\backslash{\$}, which relates the dimensionless kinetic boundary thickness with the square root of the wind Reynolds number, to fix the Reynolds number dependence. Meanwhile the theory is, on the one hand, well-confirmed through various new experiments and numerical simulations; on the other hand, these new data points provide the basis for an updated fit in a much larger parameter space. Here we pick four well-established (and sufficiently distant) $\backslash${\$}\backslashmathit{\{}Nu{\}}(\backslashmathit{\{}Ra{\}}, \backslashmathit{\{}Pr{\}})\backslash{\$} data points and show that the resulting $\backslash${\$}\backslashmathit{\{}Nu{\}}(\backslashmathit{\{}Ra{\}}, \backslashmathit{\{}Pr{\}})\backslash{\$} function is in agreement with almost all established experimental and numerical data up to the ultimate regime of thermal convection, whose onset also follows from the theory. One extra $\backslash${\$}\backslashmathit{\{}Re{\}}(\backslashmathit{\{}Ra{\}}, \backslashmathit{\{}Pr{\}})\backslash{\$} data point is used to fix $\backslash${\$}\backslashmathit{\{}Re{\}}(\backslashmathit{\{}Ra{\}}, \backslashmathit{\{}Pr{\}})\backslash{\$}. As $\backslash${\$}\backslashmathit{\{}Re{\}}\backslash{\$} can depend on the definition and the aspect ratio, the transformation properties of the GL equations are discussed in order to show how the GL coefficients can easily be adapted to new Reynolds number data while keeping $\backslash${\$}\backslashmathit{\{}Nu{\}}(\backslashmathit{\{}Ra{\}}, \backslashmathit{\{}Pr{\}})\backslash{\$} unchanged.},
archivePrefix = {arXiv},
